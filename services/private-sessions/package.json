{
  "name": "@speakeasy-services/private-sessions",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "prebuild": "prisma generate",
    "build": "tsc --build",
    "dev": "concurrently \"pnpm dev:prisma\" \"pnpm dev:api\" \"pnpm dev:worker\"",
    "dev:prisma": "prisma generate --watch",
    "dev:api": "tsx watch src/api.ts",
    "dev:api:debug": "tsx watch  --onError quit src/api.ts",
    "dev:worker": "tsx watch src/worker.ts",
    "dev:watch": "nodemon --watch ../../packages/service-base/dist --watch src -e js,ts --exec 'tsx src/index.ts'",
    "start:api": "node dist/api.js",
    "start:worker": "node dist/worker.js",
    "clean": "rm -rf dist",
    "lint": "eslint src",
    "test": "jest",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate deploy"
  },
  "dependencies": {
    "@atproto/lexicon": "^0.4.9",
    "@atproto/xrpc": "^0.6.11",
    "@atproto/xrpc-server": "^0.7.13",
    "@prisma/client": "^6.5.0",
    "@speakeasy-services/common": "workspace:*",
    "@speakeasy-services/crypto": "workspace:*",
    "@speakeasy-services/queue": "workspace:*",
    "@speakeasy-services/service-base": "workspace:^",
    "dotenv": "^16.4.7",
    "fastify": "^5.2.1",
    "node-cache": "^5.1.2",
    "node-fetch": "^3.3.2",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/node": "^22.13.11",
    "@types/node-cache": "^4.2.5",
    "concurrently": "^8.2.2",
    "prisma": "^6.5.0",
    "tsup": "^8.4.0",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2"
  }
}
