{
  "name": "@speakeasy-services/trusted-users",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./lexicon": "./dist/lexicon/index.js"
  },
  "scripts": {
    "prebuild": "prisma generate",
    "build": "prisma generate && tsc --build && cp -r src/generated dist",
    "typecheck": "tsc --noEmit",
    "dev": "tsx watch src/api.ts",
    "dev:prisma": "prisma generate --watch",
    "dev:api": "tsx watch src/api.ts",
    "dev:worker": "tsx watch src/worker.ts",
    "clean": "rm -rf dist",
    "lint": "eslint src",
    "test": "vitest run",
    "test:watch": "vitest",
    "prisma:migrate": "prisma migrate deploy",
    "prisma:generate": "prisma generate"
  },
  "dependencies": {
    "@atproto/lexicon": "^0.4.9",
    "@atproto/xrpc": "^0.6.11",
    "@atproto/xrpc-server": "^0.7.13",
    "@prisma/client": "^6.5.0",
    "@speakeasy-services/common": "workspace:*",
    "@speakeasy-services/crypto": "workspace:*",
    "@speakeasy-services/queue": "workspace:*",
    "@speakeasy-services/service-base": "workspace:*",
    "dotenv": "^16.4.7",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@mermaid-js/mermaid-cli": "^11.4.2",
    "@speakeasy-services/test-utils": "workspace:^",
    "@types/node": "^22.13.11",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "@vitest/ui": "^3.2.4",
    "eslint": "^8.56.0",
    "prisma": "^6.5.0",
    "prisma-erd-generator": "^2.0.4",
    "supertest": "^6.3.4",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3",
    "vite": "^7.0.5",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  }
}
